{
  "name": "@vida0905/eslint-config",
  "type": "module",
  "version": "2.1.2",
  "packageManager": "pnpm@10.7.1",
  "description": "Vida Xie's ESLint Config",
  "author": "Vida Xie <vida_2020@163.com> (https://github.com/9romise/)",
  "license": "MIT",
  "homepage": "https://github.com/9romise/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dits/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typegen": "tsx scripts/typegen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "check": "npm run typecheck && npm run lint",
    "prepack": "npm run build",
    "prepare": "npm run typegen && simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=9.10.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "ansis": "^3.17.0",
    "cac": "^6.7.14",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-import-resolver-oxc": "^0.13.1",
    "eslint-plugin-de-morgan": "^1.2.1",
    "eslint-plugin-pinia": "^0.4.1",
    "local-pkg": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "eslint": "^9.23.0",
    "eslint-typegen": "^2.1.0",
    "nano-staged": "^0.8.0",
    "oxc-transform": "^0.62.0",
    "simple-git-hooks": "^2.12.1",
    "tsdown": "^0.6.10",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
