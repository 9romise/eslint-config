{
  "name": "@vida0905/eslint-config",
  "type": "module",
  "version": "1.2.0",
  "packageManager": "pnpm@9.15.3",
  "description": "Vida Xie's ESLint Config",
  "author": "Vida Xie <vida_2020@163.com> (https://github.com/9romise/)",
  "license": "MIT",
  "homepage": "https://github.com/9romise/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dits/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "tsup --watch",
    "typegen": "tsx scripts/typegen",
    "build": "npm run typegen && tsup",
    "release": "npm run lint && npm run typecheck && bumpp",
    "typecheck": "tsc --noEmit",
    "prepack": "npm run build",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=9.10.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "eslint-import-resolver-oxc": "^0.8.0",
    "eslint-plugin-pinia": "^0.4.1",
    "local-pkg": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "bumpp": "^9.10.0",
    "eslint": "^9.18.0",
    "eslint-typegen": "^1.0.0",
    "lint-staged": "^15.3.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
