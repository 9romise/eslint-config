{
  "name": "@vida0905/eslint-config",
  "type": "module",
  "version": "2.1.1",
  "packageManager": "pnpm@10.4.1",
  "description": "Vida Xie's ESLint Config",
  "author": "Vida Xie <vida_2020@163.com> (https://github.com/9romise/)",
  "license": "MIT",
  "homepage": "https://github.com/9romise/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dits/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typegen": "tsx scripts/typegen",
    "check": "tsc --noEmit && eslint",
    "prepack": "npm run build",
    "prepare": "npm run typegen && simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=9.10.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "ansis": "^3.15.0",
    "cac": "^6.7.14",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-import-resolver-oxc": "^0.11.0",
    "eslint-plugin-de-morgan": "^1.0.1",
    "eslint-plugin-pinia": "^0.4.1",
    "local-pkg": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "eslint-typegen": "^2.0.0",
    "nano-staged": "^0.8.0",
    "oxc-transform": "^0.51.0",
    "simple-git-hooks": "^2.11.1",
    "tsdown": "^0.5.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
