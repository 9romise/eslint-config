{
  "name": "@vida0905/eslint-config",
  "type": "module",
  "version": "2.7.1",
  "packageManager": "pnpm@10.24.0",
  "description": "Vida Xie's ESLint Config",
  "author": "Vida Xie <vida_2020@163.com> (https://github.com/9romise/)",
  "license": "MIT",
  "homepage": "https://github.com/9romise/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dits/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "./bin/index.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typegen": "tsx scripts/typegen",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint .",
    "check": "npm run typecheck && npm run lint",
    "prepare": "npm run typegen && npm run build",
    "inspect": "npx eslint --inspect-config --config eslint-inspector.config.ts"
  },
  "peerDependencies": {
    "@nuxt/eslint-plugin": "*",
    "@unocss/eslint-plugin": "*",
    "eslint": ">=9.10.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/eslint-plugin": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "catalog:main",
    "cac": "catalog:cli",
    "eslint-flat-config-utils": "catalog:main",
    "eslint-plugin-de-morgan": "catalog:plugin",
    "local-pkg": "catalog:main"
  },
  "devDependencies": {
    "@types/node": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-typegen": "catalog:dev",
    "husky": "catalog:dev",
    "nano-staged": "catalog:dev",
    "tsdown": "catalog:dev",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev",
    "vitest": "catalog:test"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
