{
  "name": "@vida0905/eslint-config",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@10.2.0",
  "description": "Vida Xie's ESLint Config",
  "author": "Vida Xie <vida_2020@163.com> (https://github.com/9romise/)",
  "license": "MIT",
  "homepage": "https://github.com/9romise/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dits/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "tsup --watch",
    "typegen": "tsx scripts/typegen",
    "build": "npm run typegen && tsup",
    "release": "npm run lint && npm run typecheck && bumpp",
    "typecheck": "tsc --noEmit",
    "prepack": "npm run build",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=9.10.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-import-resolver-oxc": "^0.10.1",
    "eslint-plugin-pinia": "^0.4.1",
    "local-pkg": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.0.0",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.2",
    "eslint": "^9.19.0",
    "eslint-typegen": "^1.0.0",
    "lint-staged": "^15.4.3",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
