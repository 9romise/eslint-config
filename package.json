{
  "name": "@vida0905/eslint-config",
  "type": "module",
  "version": "0.5.0",
  "packageManager": "pnpm@9.7.0",
  "description": "Vida Xie's ESLint Config",
  "author": "Vida Xie <vida_2020@163.com> (https://github.com/9romise/)",
  "license": "MIT",
  "homepage": "https://github.com/9romise/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9romise/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/9romise/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dits/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "tsup --watch",
    "inspect": "npx @eslint/config-inspector",
    "typegen": "tsx scripts/typegen",
    "build": "npm run typegen && tsup",
    "release": "npm run lint && npm run typecheck && bumpp",
    "typecheck": "tsc --noEmit",
    "prepack": "npm run build",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": ">=8.40.0",
    "eslint-plugin-format": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "eslint-import-resolver-oxc": "^0.2.0",
    "eslint-plugin-pinia": "^0.2.0",
    "local-pkg": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.2",
    "@types/node": "^22.1.0",
    "bumpp": "^9.4.2",
    "eslint": "^9.8.0",
    "eslint-typegen": "^0.3.0",
    "lint-staged": "^15.2.8",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
